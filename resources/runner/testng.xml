<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Page Object Model">
   
   <!-- To generate ReportNG you need to add below two lines -->
   <listeners>
   		<listener class-name="org.uncommons.reportng.HTMLReporter"/>
   		<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
   		<listener class-name="com.bt.listeners.Customelisteners"/>
   </listeners>
   
   
   <test thread-count="1" name="Login Test">
    <classes>
      <class name="com.bt.testcases.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  
  
  
</suite> <!-- Suite -->
